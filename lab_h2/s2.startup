/etc/init.d/networking restart
echo -e '\n\n\n' | ssh-keygen
pwd
cp /root/.ssh/id_rsa.pub /hostlab

/etc/init.d/ssh restart
mkdir /home/ssh_user
useradd ssh_user -d /home/ssh_user
echo -e 'user\nuser\n' | passwd ssh_user

#!/bin/bash
/usr/bin/expect << EOF
set timeout -1
ssh -NR 9000:10.0.1.18:8000 homework_user@10.0.0.100
expect "Are you sure you want to continue connecting (yes/no)?" {send "yes\n"}
expect "ssh_user@r4's password:*" {send "user\n"}
interact